  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: FastAPI",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "cwd": "${workspaceFolder}/src/api",
      "args": ["main:app", "--reload"],
      "jinja": true
    },
    {
      "name": "Python: Orchestrator",
      "type": "debugpy",
      "request": "launch",
      "module": "api.agents.orchestrator",
      "cwd": "${workspaceFolder}/src/api",
      "justMyCode": true
    },
    {
      "name": "Python: Evaluate",
      "type": "debugpy",
      "request": "launch",
      "module": "evaluate.evaluate",
      "cwd": "${workspaceFolder}/src/api",
      "justMyCode": true
    }
  ]
}
